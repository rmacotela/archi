              <child xsi:type="archimate:DiagramObject" id="id-ff26fe97d4064db7afa97fb155779187" targetConnections="id-ef2503bc9b0e45b98172726f0906378b id-8b8f5bbf84c541ddb887a4357dc1e038 id-11c887a4db0e44f4ae33226976781732" archimateElement="id-c8f40f70f6554da08f6446a81dfd1d4d" type="1">
